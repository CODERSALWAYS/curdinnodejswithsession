						<tr>
							<td><%=data[i].message%></td>
							<td ><a href="/update?updateid=<%=data[i].mid%>">
								<span class="fa fa-edit"></a></span>
								<a href=""></a>
							</td>
							<td><a href="/delete?deleteid=<%=data[i].mid%>">
								<span class="fa fa-trash"></span></a></td>
						</tr>
							<%} %>
					</table>
				</div>
